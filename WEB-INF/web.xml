<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>Top.html</welcome-file>
  </welcome-file-list>
  <error-page>
    <error-code>400</error-code>
    <location>/error/400_BadRequest.html</location>
  </error-page>
  <error-page>
    <error-code>401</error-code>
    <location>/error/401_Unauthorized.html</location>
  </error-page>
  <error-page>
    <error-code>403</error-code>
    <location>/error/403_Forbidden.html</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/error/404_NotFound.html</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/error/500_InternalServerError.html</location>
  </error-page>
  <servlet>
    <servlet-name>UmamusumeList</servlet-name>
    <servlet-class>com.umamusumelist.servlet.UmamusumeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UmamusumeList</servlet-name>
    <url-pattern>/UmamusumeList</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RacingUmamusumeList</servlet-name>
    <servlet-class>com.umamusumelist.servlet.RacingUmamusumeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RacingUmamusumeList</servlet-name>
    <url-pattern>/RacingUmamusumeList</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Manager</servlet-name>
    <servlet-class>com.umamusumelist.servlet.manager.ManagerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Manager</servlet-name>
    <url-pattern>/Manager/</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SetOrDeleteUmamusume</servlet-name>
    <servlet-class>com.umamusumelist.servlet.manager.SetOrDeleteUmamusumeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SetOrDeleteUmamusume</servlet-name>
    <url-pattern>/Manager/SetOrDeleteUmamusume</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SetOrDeleteUmamusumeExclusive</servlet-name>
    <servlet-class>com.umamusumelist.servlet.manager.SetOrDeleteUmamusumeExclusiveServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SetOrDeleteUmamusumeExclusive</servlet-name>
    <url-pattern>/Manager/SetOrDeleteUmamusumeExclusive</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SetOrDeleteRacingUmamusume</servlet-name>
    <servlet-class>com.umamusumelist.servlet.manager.SetOrDeleteRacingUmamusumeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SetOrDeleteRacingUmamusume</servlet-name>
    <url-pattern>/Manager/SetOrDeleteRacingUmamusume</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SetOrDeleteNotUmamusume</servlet-name>
    <servlet-class>com.umamusumelist.servlet.manager.SetOrDeleteNotUmamusumeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SetOrDeleteNotUmamusume</servlet-name>
    <url-pattern>/Manager/SetOrDeleteNotUmamusume</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>MyFilter</filter-name>
    <filter-class>com.umamusumelist.filter.MyFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>MyFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <session-config>
    <session-timeout>2</session-timeout>
  </session-config>
</web-app>